{
    "name": "fbhappy",
    "version": "0.0.2",
    "manifest_version": 2,
    "description": "Making you happy with your FB",
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "default_locale": "en",
    "browser_action": {
        "default_icon": "images/icon-19.png",
        "default_title": "fbhappy"
    },
    "content_scripts": [
      {
        "matches": ["https://www.facebook.com/*"],
        "css": ["styles/fbhappy.css"]
      }
    ]
}
